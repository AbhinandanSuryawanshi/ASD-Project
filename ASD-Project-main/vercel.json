{
  "buildCommand": "npm run build",
  "devCommand": "npm start",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "create-react-app",
  "env": [
    {
      "key": "REACT_APP_BACKEND_URL",
      "value": "http://localhost:8000",
      "type": "plaintext"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
